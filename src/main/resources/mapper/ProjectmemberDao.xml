<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.xchuiao.projectprocedurems.dao.ProjectmemberDao">

    <resultMap type="top.xchuiao.projectprocedurems.entity.Projectmember" id="ProjectmemberMap">
        <result property="proId" column="pro_id" jdbcType="VARCHAR"/>
        <result property="id" column="id" jdbcType="VARCHAR"/>
    </resultMap>

    <!--查询单个-->
    <select id="queryById" resultMap="ProjectmemberMap">
        select
          pro_id, id
        from project_procedure_ms.projectmember
        where pro_id = #{proId}
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="ProjectmemberMap">
        select
          pro_id, id
        from project_procedure_ms.projectmember
        limit #{offset}, #{limit}
    </select>

    <!--通过实体作为筛选条件查询-->
    <select id="queryAll" resultMap="ProjectmemberMap">
        select
          pro_id, id
        from project_procedure_ms.projectmember
        <where>
            <if test="proId != null and proId != ''">
                and pro_id = #{proId}
            </if>
            <if test="id != null and id != ''">
                and id = #{id}
            </if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="proId" useGeneratedKeys="true">
        insert into project_procedure_ms.projectmember()
        values ()
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update project_procedure_ms.projectmember
        <set>
        </set>
        where pro_id = #{proId}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from project_procedure_ms.projectmember where pro_id = #{proId}
    </delete>

</mapper>